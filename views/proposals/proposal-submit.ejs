<!DOCTYPE html>
<html>
<%-include ../base/checkHead.ejs%>
<style>
    .container{
        padding: 42px 0 0 0;
    }
    .wrap{
        padding: 0 10%;
        display: flex;
        align-items: center;
        overflow: hidden;
    }
    .list{
        padding: 0 0 10% 0;
        width: 100%;
    }
    .list li{
        border-bottom: 1px solid #adaaaa;
        padding: 7% 10px;
        text-align: center;
    }
    .list li a{
        display: inline-block;
        white-space: nowrap;
    }
    .list li a .left{
        width: 70px;
        text-align: left;
        display: inline-block;
    }
    .list li a .left img{
        width: 60px;
        vertical-align: middle;
    }
    .list li a .right{
        color: #000;
        font-size: 18px;
        font-weight: bold;
        display: inline-block;
        width: 100px;
        text-align: left;
    }
</style>
<body>
<div class="container">
    <%-include ../base/header.ejs%>
    <div class="wrap">
        <ul class="list">
            <li>
                <a href="/proposalDown">
                    <span class="left">
                        <img src="images/ta_bxz.png">
                    </span>
                    <span class="right">提案表下载</span>
                </a>
            </li>
            <li id="subProposal">
                <a href="/submitProposal">
                    <span class="left">
                        <img src="images/ta_tj.png">
                    </span>
                    <span class="right">提案提交</span>
                </a>
            </li>
            <li>
                <a href="/proposalManage?tabIndex=0">
                    <span class="left">
                        <img src="images/ta_gl.png">
                    </span>
                    <span class="right">提案管理</span>
                </a>
            </li>
        </ul>
    </div>
</div>
</body>
<script>
    $(function () {
        if (role == 4 || role == 5) {
            $("#subProposal").css("display", "none")
        }
    })
</script>
</html>
